{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {
    "max-line-length": {
      "options": [80]
    }
  },
  "rules": {
    "ter-indent": [true, 4],
    "import-name": false,
    "max-line-length": [true, 120],
    "object-shorthand-properties-first": false,
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "examples/*"
    ]
  }
}
